{
  "canisters": {
    "my-motoko-project-backend": {
      "main": "src/my-motoko-project-backend/main.mo",
      "type": "motoko"
    },
    "my-motoko-project-frontend": {
      "dependencies": ["my-motoko-project-backend"],
      "source": ["src/my-motoko-project-frontend/dist"],
      "type": "assets",
      "workspace": "my-motoko-project-frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "type": "persistent",
      "providers": ["https://ic0.app"]
    }
  }
}
